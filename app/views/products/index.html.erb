<ul class="navbar-nav mr-auto" style="position: absolute; left: -300px; top: -130px">
        <li><%= link_to "Create Rent",  new_product_path %></li>
</ul>

<h1>Blog Posts</h1>
<div class="card-deck">
<% @products.each do |product| %>
	<div class="card">
	<div class="well", style="text-align: center">
	<h3><%= product.headline %></h3>
	<h1>Price: <%= product.price %>$</h1>
	<h3>Rooms: <%= product.rooms %></h3>
	<%= link_to "Read More", product_path(product), :class => 'btn btn-default' %>
	</div>
	</div>
	<hr>
<% end %>
</div>